FROM debian

RUN apt update
RUN apt install wget gnupg apt-transport-https default-jre -y
RUN wget -q -O - https://pkg.jenkins.io/debian-stable/jenkins.io.key | apt-key add -
RUN echo  "deb https://pkg.jenkins.io/debian-stable binary/" >> /etc/apt/sources.list
RUN apt update
RUN apt install jenkins -y

CMD  ["/etc/init.d","restart","jenkins"]
